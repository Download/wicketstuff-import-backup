<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	  "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<display-name>Wicket YUI Examples</display-name>

	<distributable />

	<context-param>
		<param-name>configuration</param-name>
		<param-value>development</param-value>
	</context-param>

	<filter>
		<filter-name>YuiApplication</filter-name>
		<filter-class>wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
			<param-name>applicationClassName</param-name>
			<param-value>
				wicket.contrib.yui.examples.pages.YuiApplication
			</param-value>
		</init-param>
		<init-param>
			<param-name>filterPath</param-name>
			<param-value>yui</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>YuiApplication</filter-name>
		<url-pattern>/yui/*</url-pattern>
	</filter-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

</web-app>
