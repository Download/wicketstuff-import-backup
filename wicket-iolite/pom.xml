<?xml version="1.0" encoding="UTF-8"?>
<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.wicketstuff.iolite</groupId>
  <artifactId>wicketstuff-iolite</artifactId>
   <packaging>maven-archetype</packaging>
  <name>wicketstuff-iolite</name>
  <version>0.3-SNAPSHOT</version>
  <build>
    <extensions>
      <extension>
        <groupId>org.apache.maven.archetype</groupId>
        <artifactId>archetype-packaging</artifactId>
        <version>2.0-alpha-2</version>
      </extension>
    </extensions>
    <plugins>
      <plugin>
        <artifactId>maven-archetype-plugin</artifactId>
        <version>2.0-alpha-2</version>
        <extensions>true</extensions>
      </plugin>
    </plugins>
  </build>
      <distributionManagement>
        <snapshotRepository>
            <id>repo</id>
            <name>Local TeamCity/Tomcat repository</name>
            <url>file:/home/wicket/tomcat/webapps/maven/repository/</url>
            <uniqueVersion>false</uniqueVersion>
        </snapshotRepository>
        <repository>
            <id>repo</id> 
            <name>Local TeamCity/Tomcat repository</name>
		    <url>file:/home/wicket/tomcat/webapps/maven/repository/</url>
        </repository>
    </distributionManagement>

</project>
