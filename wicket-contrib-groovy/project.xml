<?xml version="1.0" encoding="UTF-8"?>

<project>
	<pomVersion>3</pomVersion>
	<groupId>wicket-stuff</groupId>
	<id>wicket-contrib-groovy</id>
	<name>Wicket Contrib Groovy</name>
	<currentVersion>1.0</currentVersion>
	<organization>
		<name>Wicket Stuff</name>
		<url>http://wicket-stuff.sourceforge.net</url>
		<logo>http://wicket-stuff.sourceforge.net/images/projectLogo.png</logo>
	</organization>
	<inceptionYear>2005</inceptionYear>
	<package>wicket</package>
	<logo>/images/projectLogo.png</logo>
	<description><![CDATA[The Wicket Contrib Groovy integration project allows you to use
				<a href="http://groovy.codehaus.org">Groovy</a> (a JVM scripting language),
				to create Wicket pages. Instead of creating Java classes to match your
				HTML, you create Groovy classes.]]></description>
	<shortDescription>Integration of Groovy into Wicket as implementation language.</shortDescription>
	<url>http://wicket-stuff.sourceforge.net/${pom.artifactId}</url>
	<issueTrackingUrl>
		http://sourceforge.net/tracker/?group_id=${maven.sourceforge.project.groupId}
	</issueTrackingUrl>
	<siteAddress>shell.sourceforge.net</siteAddress>
	<siteDirectory>/home/groups/w/wi/wicket-stuff/htdocs/${pom.artifactId}</siteDirectory>
	<repository>
		<connection>
			scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/${pom.groupId}:${pom.artifactId}
		</connection>
		<developerConnection>
			scm:cvs:ext:${maven.username}@cvs.sourceforge.net:/cvsroot/${pom.groupId}:${pom.artifactId}
		</developerConnection>
		<url>http://cvs.sourceforge.net/viewcvs.py/${pom.groupId}/${pom.artifactId}</url>
	</repository>
