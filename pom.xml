<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.wicketstuff</groupId>
  <artifactId>wicketstuff-objectautocomplete-parent</artifactId>
  <packaging>pom</packaging>
  <version>1.0.0-SNAPSHOT</version>

  <name>wicketstuff-objectautocomplete-parent</name>
  <description>
    Wicket component for selecting entities via autocomplete
  </description>

  <url>http://wicket-stuff.sourceforge.net/wicketstuff-objectautocomplete</url>
  <inceptionYear>2008</inceptionYear>

  <parent>
    <groupId>org.wicketstuff</groupId>
    <artifactId>wicketstuff-parent</artifactId>
    <version>2</version>
  </parent>

   <distributionManagement>
    <repository>
      <id>repository-ftp-consol</id>
      <url>ftp://build.int.consol.de/proximity/inhouse/storage</url>
    </repository>
    <snapshotRepository>
      <id>repository-ftp-consol</id>
      <url>ftp://build.int.consol.de/proximity/inhouse.snapshot/storage</url>
    </snapshotRepository>
  </distributionManagement>

  <modules>
    <module>objectautocomplete</module>
    <module>example</module>
  </modules>

  <properties>
    <java.src.version>1.5</java.src.version>
    <wicket.version>1.4-SNAPSHOT</wicket.version>
    <slf4j.version>1.4.3</slf4j.version>
		<runtime.log>target/velocity.log</runtime.log>
	</properties>

  <developers>
    <developer>
      <name>Roland Huss</name>
      <id>roland</id>
      <email>wicketstuff-objectautoselect@tichny.org</email>
    </developer>
  </developers>

  <build>
    <plugins>
      <plugin>
        <artifactId>maven-idea-plugin</artifactId>
        <configuration>
          <downloadSources>true</downloadSources>
          <linkModules>true</linkModules>
          <jdkLevel>1.5</jdkLevel>
        </configuration>
      </plugin>
    </plugins>

    <extensions>
      <extension>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-ftp</artifactId>
        <version>1.0-beta-2</version>
      </extension>
    </extensions>
  </build>

</project>
