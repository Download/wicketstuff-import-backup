<?xml version="1.0" encoding="UTF-8"?>

<project>
	<pomVersion>3</pomVersion>
	<groupId>wicket-stuff</groupId>
	<id>wicket-contrib-freemarker</id>
	<name>FreeMarker Panel</name>
	<currentVersion>1.0</currentVersion>
	<organization>
		<name>Wicket Stuff</name>
		<url>http://wicket-stuff.sourceforge.net</url>
		<logo>http://wicket-stuff.sourceforge.net/images/projectLogo.png</logo>
	</organization>
	<inceptionYear>2005</inceptionYear>
	<package>wicket</package>
	<logo>/images/projectLogo.png</logo>
	<description>
		The FreeMarker Panel component allows you to use Freemarker (http://www.freemarker.org/)
		templates in your Wicket applications.
	</description>
	<shortDescription>Panel component for FreeMarker/Wicket integration</shortDescription>
	<url>http://wicket-stuff.sourceforge.net/wicket-freemarker-panel</url>
	<issueTrackingUrl>
		http://sourceforge.net/tracker/?group_id=${maven.sourceforge.project.groupId}
	</issueTrackingUrl>
	<siteAddress>shell.sourceforge.net</siteAddress>
	<siteDirectory>/home/groups/w/wi/wicket-stuff/htdocs/wicket-freemarker-panel</siteDirectory>
	<repository>
		<connection>
			scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/${pom.groupId}:wicket-contrib-freemarker
		</connection>
		<developerConnection>
			scm:cvs:ext:${maven.username}@cvs.sourceforge.net:/cvsroot/${pom.groupId}:wicket-contrib-freemarker
		</developerConnection>
		<url>http://cvs.sourceforge.net/viewcvs.py/${pom.groupId}/wicket-contrib-freemarker</url>
	</repository>
	<versions></versions>
	<mailingLists>
		<mailingList>
			<name>Wicket Stuff Dev List</name>
			<subscribe>
				http://lists.sourceforge.net/mailman/listinfo/${pom.groupId}-develop
			</subscribe>
			<unsubscribe>
				http://lists.sourceforge.net/mailman/listinfo/${pom.groupId}-develop
			</unsubscribe>
			<archive>
				http://sourceforge.net/mailarchive/forum.php?forum=${pom.groupId}-develop
			</archive>
		</mailingList>
		<mailingList>
			<name>Wicket Stuff User List</name>
			<subscribe>http://lists.sourceforge.net/mailman/listinfo/${pom.groupId}-user</subscribe>
			<unsubscribe>
				http://lists.sourceforge.net/mailman/listinfo/${pom.groupId}-user
			</unsubscribe>
			<archive>
				http://sourceforge.net/mailarchive/forum.php?forum=${pom.groupId}-user
			</archive>
		</mailingList>
		<mailingList>
			<name>Wicket Stuff CVS commit List</name>
			<subscribe>
				http://lists.sourceforge.net/mailman/listinfo/${pom.groupId}-autocvs
			</subscribe>
			<unsubscribe>
				http://lists.sourceforge.net/mailman/listinfo/${pom.groupId}-autocvs
			</unsubscribe>
			<archive>
				http://sourceforge.net/mailarchive/forum.php?forum=${pom.groupId}-autocvs
			</archive>
		</mailingList>
	</mailingLists>
	<developers>
		<developer>
			<name>Jonas Klingstedt</name>
			<id>jklingstedt</id>
			<email>jklingstedt at users.sourceforge.net</email>
		</developer>
	</developers>
	<contributors></contributors>
	<dependencies>
		<dependency>
			<groupId>ognl</groupId>
			<artifactId>ognl</artifactId>
			<version>2.6.7</version>
			<properties>
				<war.bundle>true</war.bundle>
				<wicket.group>wicket</wicket.group>
			</properties>
		</dependency>
		<dependency>
			<groupId>servletapi</groupId>
			<artifactId>servletapi</artifactId>
			<version>2.3</version>
			<properties>
				<war.bundle>false</war.bundle>
			</properties>
		</dependency>
		<dependency>
			<groupId>concurrent</groupId>
			<artifactId>concurrent</artifactId>
			<version>1.3.3</version>
			<properties>
				<war.bundle>true</war.bundle>
				<wicket.group>wicket</wicket.group>
			</properties>
		</dependency>
		<dependency>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
			<version>2.1</version>
			<properties>
				<war.bundle>true</war.bundle>
				<wicket.group>wicket</wicket.group>
			</properties>
		</dependency>
		<dependency>
			<groupId>commons-logging</groupId>
			<artifactId>commons-logging</artifactId>
			<version>1.0.4</version>
			<properties>
				<war.bundle>true</war.bundle>
				<wicket.group>wicket</wicket.group>
			</properties>
		</dependency>
		<dependency>
			<groupId>freemarker</groupId>
			<artifactId>freemarker</artifactId>
			<version>2.3.3</version>
			<properties>
				<war.bundle>true</war.bundle>
				<wicket.group>required</wicket.group>
				<wicket.description>The supported version of FreeMarker.</wicket.description>
			</properties>
			<url>http://www.freemarker.org</url>
		</dependency>
		<dependency>
			<groupId>wicket</groupId>
			<artifactId>wicket</artifactId>
			<version>1.1-b4</version>
			<properties>
				<eclipse.dependency>true</eclipse.dependency>
				<war.bundle>true</war.bundle>
				<wicket.group>required</wicket.group>
				<wicket.description>
					This the supported version of Wicket. This dependency requires other libraries
					in order to work. See the Wicket project for more information on these
					dependencies.
				</wicket.description>
			</properties>
		</dependency>
	</dependencies>
	<build>
		<sourceDirectory>src/java</sourceDirectory>
		<resources>
			<resource>
				<directory>src/java</directory>
				<includes>
					<include>**</include>
				</includes>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
				<filtering>false</filtering>
			</resource>
		</resources>
	</build>
	<reports>
		<report>maven-license-plugin</report>
		<report>maven-javadoc-plugin</report>
	</reports>
	<properties />
</project>
