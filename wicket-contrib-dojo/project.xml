  <dependencies>
    <dependency>
      <groupId>wicket</groupId>
      <artifactId>wicket</artifactId>
      <version>1.2-SNAPSHOT</version>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description>The Wicket core classes</wicket.description>
        <eclipse.dependency>true</eclipse.dependency>
      </properties>
    </dependency>
 
    <dependency>
      <groupId>jdom</groupId>
      <artifactId>jdom</artifactId>
      <version>1.0</version>
    </dependency>
    
    <dependency>
      <artifactId>rome</artifactId>
      <groupId>rome</groupId>
      <version>0.7</version>
      <id>rome</id>
    </dependency>
    <!--		
			===================================================================
			== Dependencies for using Wicket container                       ==
			===================================================================
		-->
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <url>http://www.junit.org</url>
      <properties>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for testing Wicket</wicket.description>
        <war.bundle>false</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>servletapi</groupId>
      <artifactId>servletapi</artifactId>
      <version>2.3</version>
      <jar>servletapi-2.3.jar</jar>
      <properties>
        <wicket.group>compile</wicket.group>
        <wicket.description>Compile time only, is supplied by your application server.</wicket.description>
        <war.bundle>false</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.0.4</version>
      <url>http://jakarta.apache.org/commons/logging/</url>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description>This library enables us to give you the choice of logging toolkit (JDK, Log4j, etc.)</wicket.description>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <!--		
			===================================================================
			== Dependencies for using Jetty servlet container                ==
			===================================================================
	-->

    <dependency>
      <groupId>jetty</groupId>
      <artifactId>org.mortbay.jetty</artifactId>
      <version>4.2.24</version>
      <type>jar</type>
      <url>http://www.mortbay.com</url>
      <properties>
        <war.bundle>false</war.bundle>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for running the application from within your IDE.</wicket.description>
      </properties>
    </dependency>
    <dependency>
      <groupId>tomcat</groupId>
      <artifactId>jasper-runtime</artifactId>
      <version>4.1.30</version>
      <type>jar</type>
      <properties>
        <war.bundle>false</war.bundle>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for running the application from within your IDE.</wicket.description>
      </properties>
    </dependency>
    <dependency>
      <groupId>tomcat</groupId>
      <artifactId>jasper-compiler</artifactId>
      <version>4.1.30</version>
      <type>jar</type>
      <properties>
        <war.bundle>false</war.bundle>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for running the application from within your IDE.</wicket.description>
      </properties>
    </dependency>
  </dependencies>
  <build>
    <sourceDirectory>src/main/java</sourceDirectory>
    <unitTestSourceDirectory>src/test/java</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
      <resources>
        <resource>
          <directory>src/test/java</directory>
          <includes>
            <include>**</include>
          </includes>
          <excludes>
            <exclude>**/*.java</exclude>
          </excludes>
          <filtering>false</filtering>
        </resource>
      </resources>
    </unitTest>
    <resources>
      <resource>
        <directory>src/main/java</directory>
        <includes>
          <include>**</include>
        </includes>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
        <filtering>false</filtering>
      </resource>
      <resource>
        <directory>src/test/java</directory>
        <includes>
          <include>**</include>
        </includes>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
        <filtering>false</filtering>
      </resource>
    </resources>
  </build>
  <reports>
    <report>maven-license-plugin</report>
    <report>maven-javadoc-plugin</report>
    <report>maven-changes-plugin</report>
  </reports>
  <properties/>
</project>
