<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
      "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<display-name>Wicket JasperReports Examples</display-name>

    <filter>
        <filter-name>JasperReportsApplication</filter-name>
        <filter-class>wicket.protocol.http.WicketFilter</filter-class>
        <init-param>
          <param-name>applicationClassName</param-name>
          <param-value>wicket.contrib.jasperreports.examples.JasperReportsApplication</param-value>
        </init-param>
    </filter>

    <filter-mapping>
        <filter-name>JasperReportsApplication</filter-name>
        <url-pattern>/app/*</url-pattern>
    </filter-mapping>
 
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>