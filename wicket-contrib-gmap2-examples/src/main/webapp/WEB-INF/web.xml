<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <display-name>Wicket Examples</display-name>

	<filter>
		<filter-name>ControlsGMapApplication</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
		  <param-name>applicationClassName</param-name>
		  <param-value>wicket.contrib.examples.gmap.controls.ControlsGMapApplication</param-value>
		</init-param>
	</filter>

	<filter>
		<filter-name>TopGMapApplication</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
		  <param-name>applicationClassName</param-name>
		  <param-value>wicket.contrib.examples.gmap.top.TopGMapApplication</param-value>
		</init-param>
	</filter>

	<filter>
		<filter-name>GeoCodeGMapApplication</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
		  <param-name>applicationClassName</param-name>
		  <param-value>wicket.contrib.examples.gmap.geocode.GeoCodeGMapApplication</param-value>
		</init-param>
	</filter>

	<filter>
		<filter-name>BottomGMapApplication</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
		  <param-name>applicationClassName</param-name>
		  <param-value>wicket.contrib.examples.gmap.bottom.BottomGMapApplication</param-value>
		</init-param>
	</filter>

	<filter>
		<filter-name>SimpleGMapApplication</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
		  <param-name>applicationClassName</param-name>
		  <param-value>wicket.contrib.examples.gmap.simple.SimpleGMapApplication</param-value>
		</init-param>
	</filter>

	<filter>
		<filter-name>PolygonesGMapApplication</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
		  <param-name>applicationClassName</param-name>
		  <param-value>wicket.contrib.examples.gmap.polygones.PolygonesGMapApplication</param-value>
		</init-param>
	</filter>

	<filter>
		<filter-name>ManyGMapsApplication</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
		  <param-name>applicationClassName</param-name>
		  <param-value>wicket.contrib.examples.gmap.many.ManyGMapsApplication</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>GeoCodeGMapApplication</filter-name>
		<url-pattern>/geocode/*</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>ControlsGMapApplication</filter-name>
		<url-pattern>/controls/*</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>TopGMapApplication</filter-name>
		<url-pattern>/top/*</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>BottomGMapApplication</filter-name>
		<url-pattern>/gmap2/*</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>SimpleGMapApplication</filter-name>
		<url-pattern>/simple/*</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>PolygonesGMapApplication</filter-name>
		<url-pattern>/polygones/*</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>ManyGMapsApplication</filter-name>
		<url-pattern>/many/*</url-pattern>
	</filter-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
